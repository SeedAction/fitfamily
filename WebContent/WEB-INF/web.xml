<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FitFamily</display-name>
  
  <!-- The Home servlet - BEGIN -->
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<servlet-class>org.FitFamily.controller.HomeController</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <!-- The Home servlet - END -->
  
  <!-- The SignUp servlet - BEGIN -->
  <servlet>
  	<servlet-name>SignUp</servlet-name>
  	<servlet-class>org.FitFamily.controller.SignUpController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignUp</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <!-- The Home servlet - END -->
  
  <!-- The User servlet - BEGIN -->
  <servlet>
  	<servlet-name>User</servlet-name>
  	<servlet-class>org.FitFamily.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  <!-- The User servlet - END -->
  
</web-app>